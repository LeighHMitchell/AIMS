    "create-auth-accounts": "tsx src/scripts/create-missing-auth-accounts.ts",
    "diagnose-email": "tsx src/scripts/diagnose-email-change.ts",
    "setup-local-user": "tsx src/scripts/setup-local-user.ts",
    "fix-user-auth": "tsx src/scripts/fix-user-auth-mismatch.ts",
    "reset-password": "tsx src/scripts/reset-user-password.ts",
    "debug-login": "tsx src/scripts/debug-login.ts",
    "fix-avatar-urls": "tsx src/scripts/fix-broken-avatar-urls.ts",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "dev:clean": "npm run clean && npm run dev",
    "dev:reset": "pkill -f 'next dev' || true && rm -rf .next && npm cache clean --force && npm run dev",
    "dev:hard-reset": "pkill -f node || true && rm -rf .next && rm -rf node_modules && npm cache clean --force && npm install && npm run dev",
    "validate-production": "node scripts/validate-production.js",
    "prepare-deployment": "npm run validate-production && npm run type-check && npm run lint && npm run build",
    "postinstall": "npm rebuild sharp || npm install sharp --no-save"
  },
