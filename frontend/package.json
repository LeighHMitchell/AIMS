  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "//": "Deploy trigger - force Vercel rebuild v4 - Production ready with GitHub Actions",
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev-safe": "NODE_OPTIONS=\"--max-old-space-size=4096\" next dev -H 0.0.0.0",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "deploy": "bash scripts/deploy.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "precommit": "npm run type-check && npm run lint",
    "create-test-users": "tsx src/scripts/create-test-users.ts",
    "sync-users": "tsx src/scripts/sync-auth-users.ts",
    "create-test-user": "tsx src/scripts/create-test-user.ts",
